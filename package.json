{
  "name": "accompany-map",
  "version": "1.0.0",
  "description": "NuxtJS (VueJS 2.x) with Mapbox and Google Sheet",
  "author": "Guillermo Croppi",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "build": "nuxt build --dotenv .env.production",
